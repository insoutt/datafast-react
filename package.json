{
  "name": "@insoutt/datafast-react",
  "version": "2.0.1",
  "description": "Base package scaffold for datafast-react.",
  "keywords": [
    "react",
    "datafast"
  ],
  "homepage": "https://github.com/insoutt/datafast-react",
  "bugs": {
    "url": "https://github.com/insoutt/datafast-react/issues"
  },
  "author": "insoutt",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/insoutt/datafast-react.git"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn build:css && tsc",
    "build:css": "npx tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "test": "vitest run",
    "dev": "vitest",
    "ci": "npm run build && npm run check-format && npm run test",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.23",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "axios": "^1.13.3",
    "class-variance-authority": "^0.7.1"
  }
}
